// 将设置放入此文件中以覆盖默认值和用户设置。
{
      // 配置 glob 模式以排除文件和文件夹。
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/**.meta": true,
    "**/build-templates": true,
    "**/library": true,
    "**/temp": true,
    "**/local": true,
    "**/packages": true,
    "**/settings": true
  },

"editor.insertSpaces": false
}
